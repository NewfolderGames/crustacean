{
	"rules": {
		"comma-dangle": ["error", "always-multiline"],
		"indent": ["error", "tab", { "SwitchCase": 1 }],
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"eol-last": ["error", "always"],
		"@typescript-eslint/no-non-null-assertion": "off"
	}
}
